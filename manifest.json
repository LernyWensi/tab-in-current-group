{
    "manifest_version": 3,
    "name": "Tab in the current group",
    "description": "Why isn't there a default shortcut for this?",
    "version": "1.0",
    "icons": {
        "16": "icons/tabs-16.png",
        "32": "icons/tabs-32.png",
        "48": "icons/tabs-48.png",
        "128": "icons/tabs-128.png"
    },
    "permissions": ["tabs", "tabGroups"],
    "background": {
        "service_worker": "service-worker.js"
    },
    "commands": {
        "New tab": {
            "description": "Open a new tab"
        },
        "New tab in the current group": {
            "description": "Open a new tab in the current tab group"
        }
    }
}
